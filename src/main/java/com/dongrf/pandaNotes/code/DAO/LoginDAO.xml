<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.dongrf.pandaNotes.code.DAO.LoginDAO">
<select id="getPassword" resultType="com.dongrf.pandaNotes.code.Entity.User"
        parameterType="String">
        select user_name,password from user t where t.USER_NAME = #{userName}
    </select>
</mapper>