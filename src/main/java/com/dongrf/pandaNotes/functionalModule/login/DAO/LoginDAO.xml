<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.dongrf.pandaNotes.functionalModule.login.DAO.LoginDAO">
    <select id="getPassword" resultType="com.dongrf.pandaNotes.functionalModule.login.JavaBean.UserBean" parameterType="string">
        select user_name,password from user_information t where t.USER_NAME = #{userName}
    </select>
</mapper>